{
  "locals": {
    "baseUrl": "/blogs",
    "url": "https://mazipan.github.io/blogs",
    "name": "Irfan Maulana",
    "owner": "Irfan Maulana",
    "description": "Irfan Maulana's Personal Blog",
    "social": {
      "github": "https://github.com/mazipan",
      "facebook": "https://www.facebook.com/mazipanneh",
      "twitter": "https://twitter.com/Maz_Ipan",
      "linkedin": "https://www.linkedin.com/in/irfanmaulanamazipan"
    }
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "wintersmith-sassify",
    "wintersmith-contents",
    "wintersmith-robots",
    "wintersmith-sitemap",
    "wintersmith-tag",
    "./node_modules/wintersmith-i18n"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "sassify": {
    "outputStyle": "compressed",
    "sourceComments": false,
    "sourceMap": false
  },
  "jade": {
    "pretty": true
  },
  "pug": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "i18n": {
    "locales": ["en", "id"],
    "defaultLocale": "id",
    "cookie": "_i18n"
  },
  "paginator": {
    "perPage": 3
  },
  "tag": {
    "perPage": 10
  },
  "jsbundler": {
    "sourceMap": false,
    "minify": true
  }
}
